{
  "name": "daks-su363",
  "version": "1.3.0",
  "description": "construction company «CD 363»",
  "repository": {
    "type": "git",
    "url": "https://github.com/daks-dev/daks-su363.git"
  },
  "author": {
    "name": "DVA",
    "email": "mail@daks.dev",
    "url": "https://daks.dev"
  },
  "homepage": "https://su363.ru",
  "private": false,
  "scripts": {
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "start": "node build/index.js",
    "cluster": "node run/cluster.js",
    "dev": "vite dev --port=3000",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "create:sitemap": "node run/create.sitemap.js",
    "create:iconset": "node run/create.iconset.js",
    "create:images": "node run/create.images.js",
    "generate:css": "npx tailwindcss -i ./static/assets/stylesheets/search.css -o ./static/assets/stylesheets/search.min.css --minify --watch",
    "build": "vite build",
    "preview": "vite preview --port=3000",
    "test": "playwright test",
    "test:unit": "vitest"
  },
  "devDependencies": {
    "@playwright/test": "^1.32.1",
    "@sveltejs/adapter-node": "^1.2.3",
    "@sveltejs/adapter-static": "^2.0.1",
    "@sveltejs/kit": "^1.14.0",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "autoprefixer": "^10.4.14",
    "classnames": "^2.3.2",
    "cssnano": "^6.0.0",
    "cssnano-preset-advanced": "^6.0.0",
    "daks-svelte": "^0.3.5",
    "dotenv": "^16.0.3",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "esm-env": "^1.0.0",
    "postcss": "^8.4.21",
    "postcss-import": "^15.1.0",
    "postcss-load-config": "^4.0.1",
    "prettier": "^2.8.7",
    "prettier-plugin-svelte": "^2.10.0",
    "svelte": "^3.57.0",
    "svelte-check": "^3.1.4",
    "tailwindcss": "^3.3.0",
    "tslib": "^2.5.0",
    "typescript": "^5.0.2",
    "vanilla-lazyload": "^17.8.3",
    "vite": "^4.2.1",
    "vite-imagetools": "^4.0.18",
    "vitest": "^0.29.8"
  },
  "type": "module"
}
